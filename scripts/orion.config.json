{
  "source": [ "./node_modules/@alaskaairux/orion-design-tokens/**/*.json" ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "./src/sass/",
      "files": [
        {
          "destination": "_TokenVariables.scss",
          "format": "scss/variables"
        }
      ]
    },
    "css": {
      "transformGroup": "scss",
      "buildPath": "./src/sass/",
      "files": [
        {
          "destination": "_TokenProperties.scss",
          "format": "css/variables"
        }
      ]
    }
  }
}